
<%- include('partials/header', { title: "Agregar Precios" } )%>


<div class="container theme-showcase" role="main">

        

<form action="guardarPrecio" method="post">

    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">ID</label>
        <div class="col-sm-6">
            <input type="text" readonly class="form-control" id="id" name="id" value="<%= id[0].next_id %>">
        </div>
    </div>


    <div class="form-group row">
        <label for="fecha" class="col-sm-2 col-form-label">Fecha</label>
        <div class="col-sm-6">
            <select class="form-control" id="fecha" name="fecha" >
                <option value ="2021-11-30">Noviembre 2021</option>
                <option value ="2021-12-31">Diciembre 2021</option>
                <option value ="2022-01-31">Enero 2022</option>
                <option value ="2022-02-28">Febrero 2022</option>
                <option value ="2022-03-31">Marzo 2022</option>
                <option value ="2022-04-30">Abril 2022</option>
                <option value ="2022-05-31">Mayo 2022</option>
                <option value ="2022-06-30">Junio 2022</option>
            </select>
        </div>
    </div>

    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Categoria Precio</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" id="categoria_precio" name="categoria_precio">
        </div>
    </div>



    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Precio Base USD/MWh</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" id="precio_base_usd_mwh" name="precio_base_usd_mwh">
        </div>
    </div>


    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Cargo Transmisi√≥n</label>
        <div class="col-sm-6">
            <input type="text"  class="form-control" id="cargo_transmicion_seguimiento_electrico" name="cargo_transmicion_seguimiento_electrico">
        </div>
    </div>


    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Precio</label>
        <div class="col-sm-6">
            <input type="text" readonly class="form-control" id="precio" name="precio">
        </div>
    </div>

    <button class="btn btn-s btn-success"  onclick= "" type="submit"> Guardar Precio  </button>
    <a class="btn btn-s btn-warning"  href="/modificarPrecios"> Cancelar  </a>
    
</div>
</form>




</body>

