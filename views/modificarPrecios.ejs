
<%- include('partials/header', { title: "Modificar Precios" } )%>


<div class="container theme-showcase" role="main">

    <table id="customers">
        <tr>
            <th>id</th>
            <th>Fecha</th>
            <th>Categoia Precio</th>
            <th>Precio Base USD/MWh</th>
            <th>Cargo Transmisi√≥n Seguimiento Electrico</th>                           
            <th>Precio</th>         
        </tr>

        <% if( precios.length > 0 ) {
            for ( var i=0 ; i <precios.length ; i++) { %>
                <tr>
                    <td><%= precios[i].id %></td>
                    <td><%= precios[i].fecha %></td>
                    <td><%= precios[i].categoria_precio %></td>
                    <td class="num"><%= precios[i].precio_base_usd_mwh %></td>
                    <td class="num"><%= precios[i].cargo_transmicion_seguimiento_electrico %></td>
                    <td class="num"><%= precios[i].precio %></td>
                    <td><button class="btn btn-xs btn-success"  onclick= " window.location = '../modificarPrecios/<%= precios[i].id %>'" > Modificar Precio  </button></td> 
                </tr>
            <% } } %>
    </table>

</div>

</body>

