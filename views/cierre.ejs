        </tr>
        
        <% if( data.recordset.length!=0 ) {
            for ( var i=0 ; i <data.recordset.length ; i++) { %>
                <tr>
                    <td><%= data.recordset[i].cms_promedio.toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$&,') %></td>
