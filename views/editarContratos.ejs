<form action="editarContrato" method="post">

    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">ID</label>
        <div class="col-sm-6">
            <input type="text" readonly class="form-control" id="id"  value="<%= contratos[0].id %>">
        </div>
    </div>


    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Fecha</label>
        <div class="col-sm-6">
            <input type="text" readonly class="form-control" id="fecha"  value="<%= contratos[0].fecha %>">
        </div>
    </div>

    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Nombre Contrato</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" id="nombre_contrato"  value="<%= contratos[0].nombre_contrato %>">
        </div>
    </div>


    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Empresa</label>
        <div class="col-sm-6">
            <input type="text" readonly class="form-control" id="empresa"  value="<%= contratos[0].empresa %>">
        </div>
    </div>

    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Potencia Contratada</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" id="potencia_contratada"  value="<%= contratos[0].potencia_contratada %>">
        </div>
    </div>


    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Categoria Precio</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" id="categoria_precio"  value="<%= contratos[0].categoria_precio %>">
        </div>
    </div>

    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Precio Base USD/MHw</label>
        <div class="col-sm-6">
            <input type="text" readonly class="form-control" id="precio_base_usd_mwh"  value="<%= contratos[0].precio_base_usd_mwh %>">
        </div>
    </div>

    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Cargo Transmisi√≥n</label>
        <div class="col-sm-6">
            <input type="text" readonly class="form-control" id="cargo_transmicion_seguimiento_electrico"  value="<%= contratos[0].cargo_transmicion_seguimiento_electrico %>">
        </div>
    </div>


    <div class="form-group row">
        <label for="id" class="col-sm-2 col-form-label">Precio</label>
        <div class="col-sm-6">
            <input type="text" readonly class="form-control" id="precio"  value="<%= contratos[0].precio  %>">
        </div>
    </div>

    <button class="btn btn-s btn-success"  onclick= "" > Guardar Contrato  </button>
    <a class="btn btn-s btn-warning"  href="/modificarContratos"> Cancelar  </a>
    
</div>
</form>




</body>

